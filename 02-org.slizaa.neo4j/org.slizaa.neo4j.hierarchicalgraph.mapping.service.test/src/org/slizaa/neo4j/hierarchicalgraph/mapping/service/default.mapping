<?xml version="1.0" encoding="ASCII"?>
<org.slizaa.neo4j.hierarchicalgraph.mapping:HierarchicalGraphMappingDescriptor xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:org.slizaa.neo4j.hierarchicalgraph.mapping="http://org/slizaa/neo4j/hierarchicalgraph/mapping">
  <rootMappings>MATCH (module:Java:Artifact) WHERE (module:Directory OR module:Jar:Archive) RETURN id(module)</rootMappings>
  <hierarchyMappings>MATCH (d:Directory)-[:CONTAINS]->(f:File) WHERE NOT (f:Type) AND NOT (f:Directory) RETURN id(d), id(f)</hierarchyMappings>
  <hierarchyMappings>MATCH (module:Java:Artifact)-[:CONTAINS]->(d:Directory)-[:CONTAINS]->(f:File) WHERE (module:Directory OR module:Jar:Archive)  AND NOT (f:Directory) RETURN DISTINCT id(module), id(d)</hierarchyMappings>
  <hierarchyMappings>MATCH (d:Directory)-[:CONTAINS]->(t:Type) RETURN id(d), id(t)</hierarchyMappings>
  <hierarchyMappings>MATCH (t:Type)-[:DECLARES]->(e) WHERE ((e:Method) OR (e:Field)) AND NOT e.visibility IS NULL RETURN id(t), id(e)</hierarchyMappings>
  <dependencyMappings mainQuery="MATCH (t1:File:Type:Java)-[r:DEPENDS_ON]->(t2:File:Type:Java) RETURN id(t1),id(t2),id(r),type(r)">
    <detailQueries>MATCH (n1)-[rel]->(n2) WHERE id(n1) in {from} AND id(n2) in {to} AND ( (n1:Type)-[rel:EXTENDS|:IMPLEMENTS]->(n2:Type) OR (n1:Method)-[rel:INVOKES]->(n2:Method) OR (n1:Method)-[rel:READS|:WRITES]->(n2:Field) OR (n1:Field)-[rel:OF_TYPE]->(n2:Type) OR (n1:Method)-[rel:THROWS]->(n2:Type) OR (n1:Method)-[rel:RETURNS]->(n2:Type) OR (n1:Method)-[rel:RETURNS]->(n2:Type) ) RETURN id(n1), id(n2), id(rel), type(rel)</detailQueries>
  </dependencyMappings>
  <labelMappers defaultImage="icons/jdt/obj16/jar_obj.png" textPropertyName="fileName">
    <requiredNeo4jLabel>Artifact</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/package_obj.png" textPropertyName="fqn">
    <requiredNeo4jLabel>Package</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/methdef_obj.png" textPropertyName="signature">
    <requiredNeo4jLabel>Method</requiredNeo4jLabel>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/field_default_obj.png" textPropertyName="name">
    <requiredNeo4jLabel>Field</requiredNeo4jLabel>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
    <propertyBasedImages propertyName="visibility" propertyValue="public" image="icons/jdt/obj16/methpub_obj.png"/>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/class_obj.png" textPropertyName="name">
    <requiredNeo4jLabel>Class</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/annotation_obj.png" textPropertyName="name">
    <requiredNeo4jLabel>Annotation</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/enum_obj.png" textPropertyName="name">
    <requiredNeo4jLabel>Enum</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/jdt/obj16/int_obj.png" textPropertyName="name">
    <requiredNeo4jLabel>Interface</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/core/obj16/fldr_obj.png" textPropertyName="fileName">
    <requiredNeo4jLabel>Directory</requiredNeo4jLabel>
  </labelMappers>
  <labelMappers defaultImage="icons/core/obj16/file_obj.png" textPropertyName="fileName">
    <requiredNeo4jLabel>File</requiredNeo4jLabel>
  </labelMappers>
</org.slizaa.neo4j.hierarchicalgraph.mapping:HierarchicalGraphMappingDescriptor>
