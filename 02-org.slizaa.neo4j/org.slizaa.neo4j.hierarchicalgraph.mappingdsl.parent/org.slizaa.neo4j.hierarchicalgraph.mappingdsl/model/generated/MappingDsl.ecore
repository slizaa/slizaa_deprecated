<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="mappingDsl" nsURI="http://www.slizaa.org/neo4j/hierarchicalgraph/mappingdsl/MappingDsl"
    nsPrefix="mappingDsl">
  <eClassifiers xsi:type="ecore:EClass" name="MappingDescriptor">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="qualifiedName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="metaInformation" eType="#//MetaInformation"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="structureDescriptor" eType="#//StructureDescriptor"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="visualisationDescriptor"
        eType="#//VisualizationDescriptor" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MetaInformation">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StructureDescriptor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="topLevelNodeQueries" eType="#//TopLevelNodesQueries"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeHierarchyQueries" eType="#//NodeHierarchyQueries"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dependencyQueries" eType="#//DependencyQueries"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TopLevelNodesQueries">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="queries" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NodeHierarchyQueries">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="queries" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DependencyQueries">
    <eStructuralFeatures xsi:type="ecore:EReference" name="simpleDependencyQueries"
        upperBound="-1" eType="#//SimpleDependencyQueries" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="aggregatedDependencyQueries"
        upperBound="-1" eType="#//AggregatedDependencyQuery" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AggregatedDependencyQuery">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="queries" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SimpleDependencyQueries">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="queries" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VisualizationDescriptor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeVisualizationDefinition"
        upperBound="-1" eType="#//NodeVisualizationDefinition" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NodeVisualizationDefinition">
    <eStructuralFeatures xsi:type="ecore:EReference" name="function" eType="#//Function"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="labelProperties" eType="#//LabelProperties"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="switch" eType="#//labelSwitch"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="labelSwitch">
    <eStructuralFeatures xsi:type="ecore:EReference" name="function" eType="#//Function"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="labelCases" upperBound="-1"
        eType="#//LabelCase" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LabelCase">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="case" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="labelProperties" eType="#//LabelProperties"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LabelProperties">
    <eStructuralFeatures xsi:type="ecore:EReference" name="textLabel" eType="#//SimpleExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="baseImage" eType="#//SimpleExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="overlayImageTopLeft" eType="#//SimpleExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="overlayImageTopRight" eType="#//SimpleExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="overlayImageBottomLeft"
        eType="#//SimpleExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="overlayImageBottomRight"
        eType="#//SimpleExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SimpleExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Function" eSuperTypes="#//SimpleExpression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
        eType="#//SimpleExpression" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
